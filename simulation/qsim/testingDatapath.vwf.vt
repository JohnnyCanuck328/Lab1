`timescale 1 ps/ 1 ps
module datapath_vlg_sample_tst(
	g_clk,
	globalRes,
	leftIn,
	pLoadD,
	pLoadL,
	pLoadR,
	rightIn,
	sampler_tx
);
input  g_clk;
input  globalRes;
input  leftIn;
input  pLoadD;
input  pLoadL;
input  pLoadR;
input  rightIn;
output sampler_tx;

reg sample;
time current_time;
always @(g_clk or globalRes or leftIn or pLoadD or pLoadL or pLoadR or rightIn)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module datapath_vlg_check_tst (
	led,
	sampler_rx
);
input [7:0] led;
input sampler_rx;

reg [7:0] led_expected;

reg [7:0] led_prev;

reg [7:0] led_expected_prev;

reg [7:0] last_led_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:1] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 1'b1;
end

// update real /o prevs

always @(trigger)
begin
	led_prev = led;
end

// update expected /o prevs

always @(trigger)
begin
	led_expected_prev = led_expected;
end


// expected led[ 7 ]
initial
begin
