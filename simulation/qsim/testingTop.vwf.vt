module top_vlg_vec_tst();
// constants                                           
// general purpose registers
reg globalClock;
reg globalReset;
reg leftIn;
reg rightIn;
// wires                                               
wire LED0;
wire LED1;
wire LED2;
wire LED3;
wire LED4;
wire LED5;
wire LED6;
wire LED7;

wire sampler;                             

// assign statements (if any)                          
top i1 (
// port map - connection between master ports and signals/registers   
	.globalClock(globalClock),
	.globalReset(globalReset),
	.LED0(LED0),
	.LED1(LED1),
	.LED2(LED2),
	.LED3(LED3),
	.LED4(LED4),
	.LED5(LED5),
	.LED6(LED6),
	.LED7(LED7),
	.leftIn(leftIn),
	.rightIn(rightIn)
);

// globalClock
always
begin
	globalClock = 1'b0;
	globalClock = #1000 1'b1;
	#1000;
end 

// globalReset
initial
begin
	globalReset = 1'b0;
	globalReset = #2000 1'b1;
end 

// leftIn
initial
begin
	leftIn = 1'b0;
	leftIn = #4000 1'b1;
	leftIn = #6000 1'b0;
	leftIn = #2000 1'b1;
	leftIn = #2000 1'b0;
end 

// rightIn
initial
begin
	rightIn = 1'b0;
	rightIn = #2000 1'b1;
	rightIn = #6000 1'b0;
end 

top_vlg_sample_tst tb_sample (
	.globalClock(globalClock),
	.globalReset(globalReset),
	.leftIn(leftIn),
	.rightIn(rightIn),
	.sampler_tx(sampler)
);

top_vlg_check_tst tb_out(
	.LED0(LED0),
	.LED1(LED1),
	.LED2(LED2),
	.LED3(LED3),
	.LED4(LED4),
	.LED5(LED5),
	.LED6(LED6),
	.LED7(LED7),
	.sampler_rx(sampler)
);
endmodule

