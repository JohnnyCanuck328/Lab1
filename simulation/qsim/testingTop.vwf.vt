`timescale 1 ps/ 1 ps
module top_vlg_sample_tst(
	globalClock,
	globalReset,
	leftIn,
	rightIn,
	sampler_tx
);
input  globalClock;
input  globalReset;
input  leftIn;
input  rightIn;
output sampler_tx;

reg sample;
time current_time;
always @(globalClock or globalReset or leftIn or rightIn)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

